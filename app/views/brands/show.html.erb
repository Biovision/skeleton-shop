<% content_for :meta_title, t('.title', name: @entity.name ) %>
<% content_for :options do %>
    <ul>
      <li><%= link_to t(:create), new_brand_path %></li>
      <li><%= link_to t(:list), brands_path %></li>
      <li><%= link_to t(:edit), edit_brand_path(@entity) %></li>
      <li><%= link_to_delete @entity %></li>
    </ul>
<% end %>

<%= render @entity %>
